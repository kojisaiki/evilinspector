{
    "name": "evilinspector",
    "displayName": "EvilInspector",
    "description": "inspect evil charactors on your code",
    "version": "1.0.6",
    "publisher": "saikou9901",
	"repository": {
		"type": "git",
		"url": "https://github.com/kojisaiki/evilinspector.git"
	},
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.inspect",
                "title": "Inspect Evils"
            },
            {
                "command": "extension.searchevil",
                "title": "Let's search evil!"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "vscode:publish": "vsce publish"
    },
    "devDependencies": {
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.0.14",
        "mocha": "^8.0.1",
        "typescript": "^3.9.5",
        "vsce": "^1.77.0",
        "vscode": "^1.1.37"
    }
}
