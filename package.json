{
    "name": "evilinspector",
    "displayName": "EvilInspector",
    "description": "inspect evil charactors on your code",
    "version": "0.0.1",
    "publisher": "saikou9901",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.inspect",
        "onCommand:extension.searchevil"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.inspect",
            "title": "Inspect Evils"
        }, {
            "command": "extension.searchevil",
            "title": "Let's search evil!"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}